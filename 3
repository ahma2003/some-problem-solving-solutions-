question : https://codeforces.com/gym/104069/problem/F


soultion : 
#include <iostream>
#include<cmath>
#include<string>
#include<vector>
#include<algorithm>

#define ll long long
using namespace std;
void fast() { ios_base::sync_with_stdio(false); cout.tie(NULL); cin.tie(NULL); }

int main() {
    fast();
    ll n, t ,a,ans=0,c;
    char x;
    cin >> n >> t;
    vector<ll>vc , vf , vp , vq;
    while (n--) {
        cin >> x >>a;
        
        if (x == 'C') {
            vc.push_back(a);
        }

        if (x == 'P') {
            vp.push_back(a);
        }

        if (x == 'F') {
            vf.push_back(a);
        }
        if (x == 'Q') {
            vq.push_back(a);
        }
    }
    sort(vc.begin(), vc.end());
    sort(vp.begin(), vp.end());
    sort(vf.begin(), vf.end());
    sort(vq.begin(), vq.end());
    c = 0;
    for (int i = 0; i < vc.size(); i++) {
        if (c + vc[i] <= t) {
            c += vc[i];
            ans++;
        }
        else {
            break;
        }
    }
    c = 0;
    for (int i = 0; i < vf.size(); i++) {
        if (c + vf[i] <= t) {
            c += vf[i];
            ans++;
        }
        else {
            break;
        }
    }
    c = 0;
    for (int i = 0; i < vp.size(); i++) {
        if (c + vp[i] <= t) {
            c += vp[i];
            ans++;
        }
        else {
            break;
        }
    }
    c = 0;
    for (int i = 0; i < vq.size(); i++) {
        if (c + vq[i] <= t) {
            c += vq[i];
            ans++;
        }
        else {
            break;
        }
    }
    cout << ans << '\n';
    return 0;
}

